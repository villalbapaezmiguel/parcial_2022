#include <string.h>
#include <ctype.h>
#include <math.h>
#include "utn.h"
#include "Albumes.h"
#define CANTIDAD 10
int main(void) {

	setbuf(stdout,NULL);
	eAlbum album[CANTIDAD];

	char letra [51];
	int estado ;
	estado = utn_pedirChar(letra, "\nIngrese algo :","\nERROR ", 51, 2);

	if(estado == 0)
	{
		printf("\nBien");
	}else{
		printf("\nmal");
	}
	printf("\n%s", letra);
//	menu(album , CANTIDAD);

	return EXIT_SUCCESS;
}
