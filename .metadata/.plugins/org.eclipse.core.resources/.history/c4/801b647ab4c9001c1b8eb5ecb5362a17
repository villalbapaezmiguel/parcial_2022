#include "Menu.h"

void menu(eGenero* genero , int tamGenero , eArtista* eArtista , int tamArtista , eTipoArtista* eTipoArtista , int tamTipoArtista , eAlbum* eAlbum , int tamAlbum )
{
	int opciones ;
	int auxLugarLibre ;

	inicilizar(genero, tamGenero, eArtista, tamArtista, eTipoArtista, tamTipoArtista, eAlbum, tamAlbum);

	do {
		printf("\n1) ALTA \n2) MODIFICACION \n3) BAJA \n4) INFORMAR \n5) LISTAR");
		printf("\nIngrese algo : ");
		scanf("%d", &opciones);

		switch(opciones)
		{
			case 1:
				printf("\nAlta");


				break;
			case 2:
				printf("\nMODIFICACION");
				break;
			case 3:
				printf("\nBAJA");
				break;
			case 4:
				printf("\nINFORMAR");
				break;
			case 5:
				printf("\nLISTAR");
				break;
			case 6 :
				printf("\nSalio del programa");
				break;
		}

	} while (opciones != 6);

}

