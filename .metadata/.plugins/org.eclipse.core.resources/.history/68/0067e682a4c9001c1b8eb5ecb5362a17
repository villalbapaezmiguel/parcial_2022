{
	int opciones ;
	int auxLugarLibre ;

	inicilizar(genero, tamGenero, eArtista, tamArtista, eTipoArtista, tamTipoArtista, eAlbum, tamAlbum);

	do {
		printf("\nIngrese algo :")

		switch(opciones)
		{
			case 1:
				break;
			case 2:
				break;
			case 3:
				break;
			case 4:
				break;
			case 5:
				break;
			case 6 :
				printf("\nSalio del programa");
				break;
		}

	} while (opciones != 6);

}

